    "@angular/platform-browser-dynamic": "5.1.2",
    "@angular/router": "5.1.2",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.6.2",
    "@angular/compiler-cli": "5.1.2",
    "@types/node": "^8.5.2",
    "codelyzer": "^4.0.2",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
