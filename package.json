{
  "name": "pinboard-pin",
  "version": "0.4.2",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --configuration production --source-map=false --output-hashing=none",
    "build:ext": "web-ext build --source-dir=dist --artifacts-dir=.",
    "build:zip": "npm run build:prod && npm run build:ext",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "web-ext run --source-dir=dist --firefox=\"C:/Program Files/Mozilla Firefox/firefox.exe\"",
    "test:dev": "web-ext run --source-dir=dist --firefox=\"C:/Program Files/Firefox Developer Edition/firefox.exe\"",
    "test:nightly": "web-ext run --source-dir=dist --firefox=\"C:/Program Files/Nightly/firefox.exe\""
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~13.3.2",
    "@angular/compiler": "~13.3.2",
    "@angular/core": "~13.3.2",
    "@angular/forms": "~13.3.2",
    "@angular/platform-browser": "~13.3.2",
    "@angular/platform-browser-dynamic": "~13.3.2",
    "@angular/router": "~13.3.2",
    "rxjs": "^7.5.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.2",
    "@angular-eslint/builder": "~13.2.0",
    "@angular-eslint/eslint-plugin": "~13.2.0",
    "@angular-eslint/eslint-plugin-template": "~13.2.0",
    "@angular-eslint/schematics": "~13.2.0",
    "@angular-eslint/template-parser": "~13.2.0",
    "@angular/cli": "~13.3.2",
    "@angular/compiler-cli": "~13.3.2",
    "@angular/language-service": "~13.3.2",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "eslint": "^8.13.0",
    "typescript": "^4.5.4",
    "web-ext": "^6.8.0",
    "web-ext-types": "^3.2.1"
  }
}
