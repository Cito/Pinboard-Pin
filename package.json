  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "build": "ng build --aot",
    "build:prod": "ng build --aot --prod --sourcemap=false && node finish",
    "build:ext": "web-ext build --source-dir=dist --artifacts-dir=.",
    "build:zip": "npm run build:prod && npm run build:ext",
    "lint": "tslint \"src/**/*.ts\"",
